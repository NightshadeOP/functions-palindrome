def is_palindrome(s):
  if len(s)<1:
    return True
  else:
    if(s[0]==s[-1]):
      print("string is : ",s)
      return is_palindrome(s[1:-1])
    else:
      return False
a=str(input("enter string : "))
if(is_palindrome(a)==True):
  print("string is palindrome")
else:
  print("string isnt a palindrome")
